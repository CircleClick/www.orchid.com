{
  "name": "orchid-www",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node import-blog.js && ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ssr": "ng build && ng run orchid-www:server",
    "serve:ssr": "node dist/orchid-www/server/main.js",
    "render:ssr": "node dist/renderer.js",
    "build:client-and-server-bundles": "ng build --configuration production && ng run orchid-www:build-server:production",
    "build:ssr:locale": "yarn run build:client-bundle:locale && yarn run build:server-bundle:locale && yarn run webpack:universal && yarn run render:ssr",
    "build:server-bundle:locale": "ng run orchid-www:build-server:production --i18n-locale $BUILD_LOCALE --i18n-format xlf --i18n-file src/i18n/messages.$BUILD_LOCALE.xlf",
    "build:client-bundle:locale": "ng build --configuration=production --i18n-locale $BUILD_LOCALE --i18n-format xlf --i18n-file src/i18n/messages.$BUILD_LOCALE.xlf",
    "extract-i18n": "ng xi18n orchid-www --i18n-format xlf --output-path i18n --i18n-locale en && ngx-extractor --input=\"src/**/*.ts\" --format=xlf --out-file=src/i18n/messages.xlf && ng run orchid-www:xliffmerge",
    "webpack:universal": "webpack --config webpack.universal.config.js --progress --colors",
    "build:static": "node import-blog.js && yarn run build:ssr && yarn run render:ssr",
    "build:static:en": "node import-blog.js && yarn run build:static",
    "build:static:ja": "BUILD_LOCALE=ja node import-blog.js && BUILD_LOCALE=ja yarn run build:ssr:locale",
    "build:static:ko": "BUILD_LOCALE=ko node import-blog.js && BUILD_LOCALE=ko yarn run build:ssr:locale",
    "build:static:zh": "BUILD_LOCALE=zh node import-blog.js && BUILD_LOCALE=zh yarn run build:ssr:locale",
    "build:static:id": "BUILD_LOCALE=id node import-blog.js && BUILD_LOCALE=id yarn run build:ssr:locale",
    "build:static:ru": "BUILD_LOCALE=ru node import-blog.js && BUILD_LOCALE=ru yarn run build:ssr:locale",
    "ngx-extractor": "ngx-extractor",
    "dev:ssr": "ng run orchid-www:serve-ssr",
    "prerender": "ng run orchid-www:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.1.3",
    "@angular/common": "^12.1.3",
    "@angular/compiler": "^12.1.3",
    "@angular/core": "^12.1.3",
    "@angular/forms": "^12.1.3",
    "@angular/localize": "12.1.3",
    "@angular/platform-browser": "^12.1.3",
    "@angular/platform-browser-dynamic": "^12.1.3",
    "@angular/platform-server": "^12.1.3",
    "@angular/router": "^12.1.3",
    "@nguniversal/express-engine": "12.1.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/i18n-polyfill": "^1.0.0",
    "core-js": "^2.6.10",
    "express": "^4.15.2",
    "node-fetch": "^2.6.0",
    "request": "^2.88.2",
    "rxjs": "~6.6.7",
    "set-value": "~>3.0.1",
    "sharp": "^0.27.2",
    "ts-loader": "^5.4.5",
    "tslib": "^2.0.0",
    "webpack-cli": "^3.3.10",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.3",
    "@angular/cli": "^12.1.3",
    "@angular/compiler-cli": "^12.1.3",
    "@angular/language-service": "^12.1.3",
    "@nguniversal/builders": "^12.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "fs-extra": "^7.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}